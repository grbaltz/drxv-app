{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "#/*": [
        "*"
      ],
      "#components/*": [
        "components/*"
      ],
      "#images/*": [
        "images/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "types": []
  },
  "include": [
    "src",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.d.ts",
    "src/custom.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
